{
    "source": {
        "include":["src"],
        "includePattern": ".js$",
        "excludePattern":"(node_modules/|docs)"
    },
    "plugins":["plugin/markdown"],
    "template":{
        "cleverLinks": true,
        "monospaceLinks": true
    },
    "opts":{
        "recurse":true,
        "destination":"./docs"
    }
}